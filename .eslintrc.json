{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-tabs":"off",
        "radix":"off",
        "import/prefer-default-export":"off",
        "no-plusplus":"off",
        "no-console":"off"
    }
}
